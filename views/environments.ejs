<%- include('partials/header') -%>
<div class="container">
  <div class="row mt-5">
      <div class="col-6">
        <ul class="row list-unstyled">

          <div class="absolute howTo">
            <h2>List of Environments</h2>
            <span>Here are a list of all possible environments! This is more for beginners.</span>
            <table>

              <tr>
                <td>Item Name</td>
                <td></td>
              </tr>
              <tr>
                <td>Item Description</td>
                <td></td>
              </tr>
              <tr>
                <td>Stack Type</td>
                <td></td>
              </tr>
            </table>

          </div>
          
          <% for(let i=0; i<ienvironments.length; i++) {%>
            <li>
              <br>
                  <h4>-<%=environments[i].name%></h4>
                  <h6>-<%=environments[i].description%></h6>
                  <h6>-<%=environments[i].enemiesFound%></h6>
                  <h6>-<%=environments[i].bossSpawns%></h6>
              </br>
            </li>

          <% } %>
          </ul>

          

      </div>  
    </div>
  </div>
</div>
</div>
<%- include('partials/footer') -%>